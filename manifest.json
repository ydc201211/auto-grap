{
  "name": "auto-grap plug",
  "version": "0.0.1",
  "manifest_version": 2,

  // 简单描述
  "description": "this is automatic grab plugin",
  "icons": {
    "16": "image/icon.png"
  },
  // 选择默认语言
  // "default_locale": "en",
  // 浏览器小图表部分
  "browser_action": {
    "default_icon": "image/icon.png",
    "default_popup": "html/popup.html"
  },

 
  "background": {
    "scripts": ["script/background.js"]
  },

  "permissions": [
      "cookies",
      "tabs",
      "activeTab",
      "notifications",
      "background",
      "storage",
      "http://vbooking.ctrip.com/*"
  ]
}